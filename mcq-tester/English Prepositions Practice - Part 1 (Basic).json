    {
      "text": "He’s interested ___ music.",
      "choices": [
        {
          "text": "on",
          "explanation": "Incorrect. We say 'interested in' to express attraction to a topic.",
          "correct": false
        },
        {
          "text": "in",
          "explanation": "Correct. 'Interested in' is the proper preposition.",
          "correct": true
        },
        {
          "text": "at",
          "explanation": "Incorrect. 'At' is not used after 'interested'.",
          "correct": false
        },
        {
          "text": "for",
          "explanation": "Incorrect. 'For' does not fit this expression.",
          "correct": false
        }
      ]
    },
