        {
          "text": "at",
          "explanation": "Incorrect. 'At' is used for exact times, not months.",
          "correct": false
        },
        {
          "text": "in",
          "explanation": "Correct. We use 'in' for months, years, and seasons.",
          "correct": true
        },
        {
          "text": "on",
          "explanation": "Incorrect. 'On' is used for specific dates or days.",
          "correct": false
        },
        {
          "text": "to",
          "explanation": "Incorrect. 'To' is not used for time expressions.",
          "correct": false
        }
      ]
